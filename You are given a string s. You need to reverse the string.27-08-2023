class Reverse
{
    // Complete the function
    // str: input string
    public static String reverseWord(String str)
    {
        // Reverse the string str
        String rstr = " ";
        char ch;
        for (int i = 0; i<str.length(); i++) {
            ch = str.charAt(i);
            
            rstr = ch + rstr;
        }
        return rstr;
    }
}
